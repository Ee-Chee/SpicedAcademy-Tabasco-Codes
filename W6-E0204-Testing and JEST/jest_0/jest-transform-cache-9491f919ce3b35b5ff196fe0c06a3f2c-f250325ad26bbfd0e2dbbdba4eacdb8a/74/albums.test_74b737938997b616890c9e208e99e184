jest.mock("./spotify");

const { getAlbumNames } = require("./albums");
const spotify = require("./spotify");test("album names are in alphabetical order", () => {
    spotify.search.mockResolvedValue({
        albums: {
            items: [{ name: "g album" }, { name: "s album" }, { name: "b album" }]
        }
    });
    return getAlbumNames("meat loaf").then(albumNames => {
        expect(albumNames).toEqual(albumNames.sort());
    });
});
